<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ChartSample.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ChartSample"
    Shell.FlyoutBehavior="Flyout"
    Title="ChartSample">

    <ShellContent Title="Category Axis"
                  ContentTemplate="{DataTemplate local:MainPage}"
                  Route="MainPage" />
    
    <ShellContent Title="DateTimeAxis"
                  ContentTemplate="{DataTemplate local:DateTimeAxisSample}"
                  Route="DateTimeAxisSample" />
    
    <ShellContent Title="DataPoint To Value"
              ContentTemplate="{DataTemplate local:DataPointToValueSample}"
              Route="DataPointToValueSample" />

</Shell>
